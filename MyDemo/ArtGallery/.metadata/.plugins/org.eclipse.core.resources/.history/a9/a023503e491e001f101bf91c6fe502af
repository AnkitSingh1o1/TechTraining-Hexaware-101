package com.model;

public class Atemp {
	
	public static void main(String[] args) {
		A a = new A();
		A.fun();
		
		B b = new B();
		b.fun();
		
		A ab = new B();
		ab.fun();
	}
}

class A{
	public static void fun() {
		System.out.println("in Atemp");
	}
}

class B extends A{
	public static void fun() {
		System.out.println("in B");
	}
}


